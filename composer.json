{
    "name": "tnetdev/magento-ce.dev",
    "authors": [
        {
            "name": "Rudger Gravestein",
            "email": "rudgergravestein@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "magento-ce": {
            "type": "vcs",
            "url": "https://github.com/AydinHassan/magento-community"
        },
        "firegento": {
            "type": "composer",
            "url": "https://packages.firegento.com"
        }
    },
    "require": {
        "magento-hackathon/magento-composer-installer": "3.0.*",
        "aydin-hassan/magento-core-composer-installer": "dev-master",
        "magento/magento": "1.9.x-dev"
    },
    "require-dev": {
        "aoepeople/aoe_profiler": "^0.3.0",
        "aoepeople/aoe_templatehints": "^0.4.3"
    },
    "extra": {
        "magento-root-dir": "htdocs",
        "auto-append-gitignore": true
    }
}
